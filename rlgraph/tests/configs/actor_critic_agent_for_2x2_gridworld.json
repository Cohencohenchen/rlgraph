{
  "type": "actor-critic",

  "sample_episodes": false,
  "discount": 0.99,
  "gae_lambda": 1.0,

  "memory_spec":
  {
    "type": "ring-buffer",
    "capacity": 64
  },

  "preprocessing_spec":
  [
  ],

  "observe_spec": {
    "buffer_size": 16
  },

  "network_spec":
  [
    {
      "type": "dense",
      "units": 10,
      "activation": "linear",
      "scope": "hidden"
    }
  ],

  "update_spec": {
    "do_updates": true,
    "update_interval": 4,
    "batch_size": 16
  },

  "optimizer_spec": {
    "type": "adam",
    "learning_rate": 0.05
  }
}
