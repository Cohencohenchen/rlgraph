{
  "type": "apex",
  "discount": 0.99,
  "memory_spec": {
    "type": "prioritized_replay",
    "capacity": 10000
  },
  "preprocessing_spec": null,
  "saver_spec": null,
  "execution_spec": {
    "session_config": {
      "log_device_placement": false,
      "allow_soft_placement": true
    },
    "ray_spec": {
      "executor_spec": {
        "redis_address": null,
        "num_cpus": 1,
        "num_gpus": 0,
        "weight_sync_steps": 32,
        "replay_sampling_task_depth": 1,
        "env_interaction_task_depth": 1,
        "num_worker_samples": 50,
        "learn_queue_size": 1,
        "num_sample_workers": 1,
        "num_replay_workers": 1
      },
      "worker_spec": {
        "execution_spec": {
          "gpu_spec": { "gpus_enabled" : false}
        },
        "n_step_adjustment": 0,
        "worker_computes_weights": true,
        "sample_exploration": true,
        "exploration_min_value": 0.5
      },
      "apex_replay_spec": {
        "memory_spec": {
          "capacity": 10000
        },
        "clip_rewards": true,
        "min_sample_memory_size": 200,
        "n_step_adjustment": 0
      }
    }
  },
  "network_spec": [
    {
      "type": "dense",
      "units": 20,
      "activation": "tanh",
      "scope": "hidden"
    }
  ],
  "exploration_spec": {
    "non_explore_behavior": "max-likelihood",
    "epsilon_spec": {
      "decay_spec": {
        "type": "linear_decay",
        "from": 1.0,
        "to": 0.05,
        "start_timestep": 0,
        "num_timesteps": 1500
      }
    }
  },
  "update_spec": {
    "do_updates": true,
    "update_interval": 4,
    "batch_size": 24,
    "synch_interval": 32
  },
  "optimizer_spec": {
    "type": "gradient_descent",
    "learning_rate": 0.001,
    "clip_grad_norm": 20.0
  }
}
