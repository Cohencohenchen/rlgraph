{
  "type": "single-impala",

  "num_workers": 4,
  "worker_sample_size": 20,

  "environment_spec":
  {
    "type": "openai-gym",
    "gym_env": "LunarLander-v2",
    "visualize": true
  },

  "feed_previous_action_through_nn": false,
  "feed_previous_reward_through_nn": false,

  "preprocessing_spec":
  [
  ],

  "fifo_queue_spec":
  {
    "capacity": 1
  },

  "network_spec":
  [
      {
        "type": "dense",
        "units": 128,
        "activation": "relu",
        "scope": "dense-layer"
      },
      {
        "type": "lstm",
        "units": 128,
        "activation": "relu",
        "scope": "lstm-layer"
      }
  ],

  "internal_states_space": {
    "type": "tuple",
    "_args": [
      {
        "type": "float",
        "shape": [128]
      },
      {
        "type": "float",
        "shape": [128]
      }
    ],
    "add_batch_rank": false
  },

  "update_spec": {
    "do_updates": true,
    "batch_size": 16
  },

  "optimizer_spec":
  {
    "type": "adam",
    "learning_rate": 0.001
  }
}
