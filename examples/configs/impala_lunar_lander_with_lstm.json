{
  "type": "single-impala",

  "num_workers": 8,
  "worker_sample_size": 30,

  "visualize": 1,

  "environment_spec":
  {
    "type": "openai-gym",
    "gym_env": "MountainCar-v0"
  },

  "feed_previous_action_through_nn": false,
  "feed_previous_reward_through_nn": false,

  "preprocessing_spec":
  [
  ],

  "fifo_queue_spec":
  {
    "capacity": 1
  },

  "network_spec":
  [
      {
        "type": "dense",
        "units": 64,
        "activation": "relu",
        "scope": "layer0"
      },
      {
        "type": "dense",
        "units": 64,
        "activation": "relu",
        "scope": "layer1"
      }
  ],

  "internal_states_space": {
    "type": "tuple",
    "_args": [
      {
        "type": "float",
        "shape": [64]
      },
      {
        "type": "float",
        "shape": [64]
      }
    ],
    "add_batch_rank": false
  },

  "update_spec": {
    "do_updates": true,
    "batch_size": 16
  },

  "optimizer_spec":
  {
    "type": "adam",
    "learning_rate": 0.000005
  }
}
