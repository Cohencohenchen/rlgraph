{
  "type": "actor_critic",
  "sample_episodes": true,
  "discount": 0.99,
  "gae_lambda": 1.0,

  "memory_spec":
  {
    "type": "ring_buffer",
    "capacity": 1000
  },

  "preprocessing_spec":
  [],

  "observe_spec": {
    "buffer_size":200
  },
  "execution_spec": {
    "seed": 15
  },

  "network_spec":
  [
    {
      "type": "dense",
      "units": 32,
      "activation": "tanh",
      "scope": "hidden1"
    },
    {
      "type": "dense",
      "units": 32,
      "activation": "tanh",
      "scope": "hidden2"
    }
  ],

  "update_spec": {
    "do_updates": true,
    "update_interval": 400,
    "batch_size": 10
  },

  "optimizer_spec": {
    "type": "adam",
    "learning_rate": 0.01
  }
}
