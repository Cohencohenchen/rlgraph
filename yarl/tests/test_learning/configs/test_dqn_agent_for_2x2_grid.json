{
  "type": "dqn",

  "memory_spec":
  {
    "type": "replay",
    "capacity": 64
  },

  "preprocessing_spec":
  [
    {
      "type": "flatten"
    }
  ],

  "network_spec":
  [
    {
      "type": "dense",
      "units": 10,
      "activation": "linear",
      "scope": "hidden-layer"
    }
  ],

  "exploration_spec":
  {
    "non_explore_behavior": "max-likelihood",
    "epsilon_spec": {
      "decay_spec": {
        "type": "linear_decay",
        "from": 1.0,
        "to": 0.2,
        "start_timestep": 0,
        "num_timesteps": 1000
      }
    }
  },

  "update_spec": {
    "do_updates": true,
    "update_interval": 4,
    "batch_size": 16
  }
}
