{
  "type": "apex",
  "memory_spec": {
    "type": "prioritized_replay",
    "capacity": 1000
  },
  "apex_replay_spec": {
    "memory_spec": {
      "capacity": 1000
    },
    "clip_rewards": true,
    "num_replay_memories": 1,
    "min_sample_memory_size": 100
  },
 "preprocessing_spec":
  [
    {
      "type": "flatten"
    }
  ],
  "execution_spec": {
    "session_config": {
      "log_device_placement": false,
      "allow_soft_placement": true
    }
  },
  "network_spec": [
    {
      "type": "dense",
      "units": 20,
      "activation": "tanh",
      "scope": "hidden"
    }
  ],
  "exploration_spec": {
    "non_explore_behavior": "max-likelihood",
    "epsilon_spec": {
      "decay_spec": {
        "type": "linear_decay",
        "from": 1.0,
        "to": 0.05,
        "start_timestep": 0,
        "num_timesteps": 1500
      }
    }
  },
  "update_spec": {
    "do_updates": true,
    "update_interval": 4,
    "batch_size": 24,
    "synch_interval": 32
  },
  "optimizer_spec": {
    "type": "gradient_descent",
    "learning_rate": 0.001,
    "clip_grad_norm": 20
  }
}
